<h1>Artists</h1>
<ul>
  <% @artists.each do |artist| %>
    <li><%= link_to artist.name, artist_songs_path(artist) %></li>
  <% end %>
</ul>
<ul>
  <% @artists.each do |artist| %>
    <% artist.songs.each do |song| %>
      <li><%= link_to song.title, artist_song_path(artist, song) %></li>
    <% end %>
  <% end %>
</ul>
